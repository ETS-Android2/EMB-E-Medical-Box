<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.embapp">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.embapp.navigationnew"
            android:label="@string/title_activity_navigationnew"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.embapp.Intro" />
        <activity android:name="com.example.embapp.Main3Activity" />
        <activity android:name="com.example.embapp.InputActivity" />
        <activity android:name="com.example.embapp.Addressfragment"/>
        <activity android:name="com.example.embapp.Input2" />
        <activity android:name="com.example.embapp.Input3" />
        <activity android:name="com.example.embapp.Input4" />
        <activity android:name="com.example.embapp.Input5" />
        <activity android:name="com.example.embapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>